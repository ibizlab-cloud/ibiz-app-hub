{
  "name": "@ibiz/rt-model-api",
  "version": "0.2.77",
  "license": "MIT",
  "description": "iBiz前端运行时模型编译库",
  "author": "iBiz",
  "main": "lib/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "watch": "unbuild && node scripts/build.mjs",
    "build": "unbuild && node scripts/build.mjs",
    "cp:temp": "cp -r ./temp/* ./src && npm run prettier && npm run lint:fix && rm -rf ./temp/*",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts' -w",
    "publish:npm": "npm run build && npm publish --access public  --registry=https://registry.npmjs.org/",
    "publish:npm-beta": "npm run build && npm publish --access public --tag beta  --registry=https://registry.npmjs.org/",
    "publish:local": "npm run build && npm publish --access public --registry=http://172.16.240.221:8081/repository/local/"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^9.0.5",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "esbuild": "0.24.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
